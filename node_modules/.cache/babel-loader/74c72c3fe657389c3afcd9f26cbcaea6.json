A7C,EAA6C,IAA7C,EAA6C,CAAvC,UAAuC,E,kCAA5B,IAAA,CAAA,OAAA,CAAQ,OAAR,CAAe,CAAf,C,GAAe;AAAA;OAAa,CAA7C,CAEI,EADJ,YAAA,CAA4C,MAA5C,EAA4C,IAA5C,EAA4C,CAAtC,UAAsC,E,kCAA3B,IAAA,CAAA,MAAA,CAAO,OAAP,CAAc,CAAd,C,GAAc;AAAA;OAAa,CAA5C,CACI,CAHN,CAII,CANN,CAMM,EACN,mBAAA,CAAA,qCAAA,CADM,G,kBAEN,YAAA,CAQE,SARF,EAQE,IARF,EAQE,WAAA,CANe,MAAA,CAAA,IAAA,CAAK,KAMpB,EANyB,UAAlB,IAAkB,EAAd;6BAFb,YAAA,CAQE,mBARF,EAQE;AAPC,UAAA,KAAK,EAAA;AAAA,YAAA,OAAA,EAAa,IAAI,CAAC,QAAL,IAAa,CAA1B;AAA0B,YAAA,MAAA,EAAe,IAAI,CAAC,QAAL,IAAa;AAAtD,WAON;AALC,UAAA,GAAG,EAAE,IAAI,CAAC,EAKX;AAJC,UAAA,OAAK,EAAA,iBAAA,MAAA;AAAA,mBAAE,MAAA,CAAA,UAAA,CAAW,IAAX,CAAF;AAAA,WAIN;AAHC,UAAA,KAAK,EAAE,IAAI,CAAC,SAGb;AAFC,UAAA,KAAK,YAAK,IAAI,CAAC,QAAL,IAAa,CAAb,GAAa,GAAb,GAAa,GAAlB,SAAsC,IAAI,CAAC,MAA3C,CAEN;AADC,UAAA,KAAK,YAAK,IAAA,CAAA,QAAA,CAAS,SAAT,CAAmB,IAAI,CAAC,IAAxB,CAAL,SAAqC,IAAI,CAAC,MAAL,GAAW,QAAY,IAAI,CAAC,MAA5B,GAAkC,EAAvE;AACN,SARF,E,IAAA,E;;AAAA,U,+CAAA,C;OAQE,CARF,E;;AAAA,OAFM,EANN;AAAA,K,CAD0B;;;;AAAA,GAA5B,C","sourcesContent":["<template>\r\n  <van-cell-group class=\"item\">\r\n    <div class=\"header-date\">\r\n      <div>{{ bill.date }}</div>\r\n      <div class=\"money\">\r\n        <span><b>支</b>{{ expense.toFixed(2) }}</span>\r\n        <span><b>收</b>{{ income.toFixed(2) }}</span>\r\n      </div>\r\n    </div>\r\n    <!-- 通过动态 class 的形式，判断是支出还是收入，分别给不同的类名 -->\r\n    <van-cell\r\n      :class=\"{ expense: item.pay_type == 1, income: item.pay_type == 2 }\"\r\n      v-for=\"item in bill.bills\"\r\n      :key=\"item.id\"\r\n      @click=\"goToDetail(item)\"\r\n      :title=\"item.type_name\"\r\n      :value=\"`${item.pay_type == 1 ? '-' : '+'}${item.amount}`\"\r\n      :label=\"`${$filters.transTime(item.date)}${item.remark ? (' | ' + item.remark) : ''}`\"\r\n    />\r\n  </van-cell-group>\r\n</template>\r\n\r\n<script>\r\nimport { computed, reactive, toRefs } from 'vue'\r\nimport { useRouter } from 'vue-router'\r\nexport default {\r\n  name: 'CardItem',\r\n  props: {\r\n    bill: {\r\n      type: Object,\r\n      default: {}\r\n    }\r\n  },\r\n  setup(props) {\r\n    const router = useRouter()\r\n    const state = reactive({\r\n      income: 0,\r\n      expense: 0\r\n    })\r\n\r\n    state.income = props.bill.bills.filter(i => i.pay_type == 2).reduce((curr, item) => {\r\n      curr += Number(item.amount)\r\n      return curr\r\n    }, 0)\r\n    state.expense = props.bill.bills.filter(i => i.pay_type == 1).reduce((curr, item) => {\r\n      curr += Number(item.amount)\r\n      return curr\r\n    }, 0)\r\n\r\n    const goToDetail = (item) => {\r\n      console.log(item.id)\r\n      router.push({\r\n        path: '/detail',\r\n        query: {\r\n          id: item.id\r\n        }\r\n      })\r\n    }\r\n\r\n    return {\r\n      ...toRefs(state),\r\n      goToDetail\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang='less' scoped>\r\n  @import url('../config/custom.less');\r\n  .item {\r\n    border-radius: 10px;\r\n    overflow: hidden;\r\n    box-shadow: 0px 0px 4px 0px rgba(0, 0, 0, 0.1);\r\n    margin-bottom: 10px;\r\n    .header-date {\r\n      height: 60px;\r\n      display: flex;\r\n      background-color: #f9f9f9;\r\n      align-items: center;\r\n      justify-content: space-between;\r\n      padding: 0 10px;\r\n      div {\r\n        color: @color-text-base;\r\n      }\r\n      .money {\r\n        span {\r\n          margin-left: 20px;\r\n          b {\r\n            color: @color-text-caption;\r\n            font-weight: normal;\r\n            background-color: #f5f5f5;\r\n            padding: 3px;\r\n            margin-right: 2px;\r\n          }\r\n        }\r\n      }\r\n    }\r\n  }\r\n</style>\r\n\r\n<style lang='less'>\r\n  @import url('../config/custom.less');\r\n  .expense {\r\n    .van-cell__value {\r\n      color: @primary!important;\r\n    }\r\n  }\r\n  .income {\r\n    .van-cell__value {\r\n      color: @text-warning!important;\r\n    }\r\n  }\r\n/**\r\n * 样式分析\r\n * 因为想要修改 vant 内部样式的话，不能在带有 scoped 属性的 style 标签内修改，\r\n * 所以要“另起炉灶”，加一个不带 scoped 的 style 标签\r\n * 之所以要这样做，是因为带上 scoped 属性的 style 标签内的样式，编译后会被加上一串 hash 值，\r\n */ \r\n</style>"],"sourceRoot":""}]}