,SAAT,GAAS;AAAoD,KAA3P,E,IAAA,E;;AAAA,M,uBAAA,CACI,CAFN,CAII,EADJ,YAAA,CAAmE,KAAnE,EAAA,WAAA,EAAmB,MAAC,gBAAA,CAAG,MAAM,CAAC,IAAI,CAAC,MAAN,CAAN,CAAoB,OAApB,CAA2B,CAA3B,KAA2B,CAA9B,CAApB,EAAkD;AAAA;AAAlD,KACI,CALN,C,CAVF,C;GAgBM,CAhBN,E;;AAAA,GAiBI,EAlBN,CAmBI,CA3BN,CAuCI,EAXJ,YAAA,CASM,KATN,EAAA,WAAA,EASM,CARJ,YAAA,CAMM,KANN,EAAA,WAAA,EAMM,CALJ,WAKI,EAJJ,YAAA,CAGM,KAHN,EAAA,WAAA,EAGM,CAFJ,YAAA,CAA2G,MAA3G,EAA2G;AAApG,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,MAAA,CAAA,aAAA,CAAa,SAAb,CAAF;AAAA,KAAA,CAA+F;AAAnE,IAAA,KAAK,EAAA,CAAC,SAAD,EAAU;AAAA,MAAA,MAAA,EAAmB,IAAA,CAAA,OAAA,IAAO;AAA1B,KAAV;AAA8D,GAA3G,EAAkG,IAAlG,EAAoG;AAAA;AAApG,GAEI,EADJ,YAAA,CAAwG,MAAxG,EAAwG;AAAjG,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,MAAA,CAAA,aAAA,CAAa,QAAb,CAAF;AAAA,KAAA,CAA4F;AAAjE,IAAA,KAAK,EAAA,CAAC,QAAD,EAAS;AAAA,MAAA,MAAA,EAAmB,IAAA,CAAA,OAAA,IAAO;AAA1B,KAAT;AAA4D,GAAxG,EAA+F,IAA/F,EAAiG;AAAA;AAAjG,GACI,CAHN,CAII,CANN,CAQI,EADJ,WACI,CATN,CAWI,EADJ,YAAA,CAAoD,mBAApD,EAAoD;AAA1C,IAAA,GAAG,EAAC,aAAsC;AAAvB,IAAA,QAAM,EAAE,MAAA,CAAA;AAAe,GAApD,E,IAAA,E;;AAAA,I,YAAA,CACI,CAxDN,C","sourcesContent":["<template>\r\n  <div class=\"data\">\r\n    <div class=\"total\">\r\n      <div class=\"time\" @click=\"monthToggle\">\r\n        <span>{{ currentMonth }}</span>\r\n        <van-icon name=\"notes-o\" />\r\n      </div>\r\n      <div v-if=\"payType=='expense'\" class=\"box\">\r\n        <div class=\"title_expense\">共支出</div>\r\n        <div class=\"money_expense\">¥{{ total_expense || 0 }}</div>\r\n        <div class=\"income\" @click=\"changeData('income')\">共收入¥{{ total_income || 0 }}</div>\r\n      </div>\r\n      <div v-if=\"payType=='income'\" class=\"box\">\r\n        <div class=\"title_income\">共收入</div>\r\n        <div class=\"money_income\">¥{{ total_income || 0 }}</div>\r\n        <div class=\"income\" @click=\"changeData('expense')\">共支出¥{{ total_expense || 0 }}</div>\r\n      </div>\r\n    </div>\r\n    <div class=\"structure\">\r\n      <div class=\"head\">\r\n        <span class=\"title\">收支构成</span>\r\n        <div class=\"tab\">\r\n          <span @click=\"changeTotalType('expense')\" class=\"expense\" :class=\"{ active: totalType == 'expense' }\">支出</span>\r\n          <span @click=\"changeTotalType('income')\" class=\"income\" :class=\"{ active: totalType == 'income' }\">收入</span>\r\n        </div>\r\n      </div>\r\n      <div class=\"content\">\r\n        <div class=\"item\" v-for=\"item in totalType == 'expense' ? expense_data : income_data\" :key=\"item.type_id\">\r\n          <div class=\"left\">\r\n            <div class=\"type\">\r\n              <span :class=\"{ expense: totalType == 'expense', income: totalType == 'income' }\">\r\n                <i class=\"iconfont\" :class=\"item.type_id ? typeMap[item.type_id].icon : 0\" />\r\n              </span>\r\n              <span>{{ item.type_name }}</span>\r\n            </div>\r\n            <div class=\"progress\">{{ Number((item.number / Number(totalType == 'expense' ? total_expense : total_income)) * 100).toFixed(2) }}%</div>\r\n          </div>\r\n          <div class=\"right\">\r\n            <div class=\"percent\">\r\n              <van-progress :percentage=\"Number((item.number / Number(totalType == 'expense' ? total_expense : total_income)) * 100)\" stroke-width=\"6px\" :show-pivot=\"false\" track-color=\"#ffffff\" :color=\" totalType == 'expense' ? '#1ee3cf' : 'rgb(236, 190, 37)' \" />\r\n            </div>\r\n            <div class=\"money\">¥{{ Number(item.number).toFixed(2) || 0 }}</div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div class=\"proportion\">\r\n      <div class=\"head\">\r\n        <span class=\"title\">收支构成</span>\r\n        <div class=\"tab\">\r\n          <span @click=\"changePieType('expense')\" class=\"expense\" :class=\"{ active: pieType == 'expense' }\">支出</span>\r\n          <span @click=\"changePieType('income')\" class=\"income\" :class=\"{ active: pieType == 'income' }\">收入</span>\r\n        </div>\r\n      </div>\r\n      <div id=\"proportion\"></div>\r\n    </div>\r\n    <PopMonth ref=\"popMonthRef\" @select=\"selectMonth\" />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, onMounted, reactive, toRefs, onUnmounted } from 'vue'\r\n// 不要使用 import echarts from 'echarts' 因为 5.0 版本的 echarts 的接口已经变成了下面这样\r\n// export { EChartsFullOption as EChartsOption, connect, disConnect, dispose, getInstanceByDom, getInstanceById, getMap, init, registerLocale, registerMap, registerTheme };\r\n// import { init } from 'echarts'\r\nimport dayjs from 'dayjs'\r\nimport PopMonth from '../components/PopMonth.vue' // 月份弹窗组件\r\nimport axios from '../utils/axios'\r\nimport { typeMap } from '../utils'\r\n\r\nlet proportionChart = null\r\nexport default {\r\n  name: 'Data',\r\n  components: {\r\n    PopMonth\r\n  },\r\n  setup() {\r\n    const popMonthRef = ref(null)\r\n    const state = reactive({\r\n      typeMap,\r\n      currentMonth: dayjs().format('YYYY-MM'), // 当前月份，默认当前时间的月份\r\n      total_expense: 0, // 当月总支出\r\n      total_income: 0, // 当月总收入\r\n      expense_data: [], // 支出数组\r\n      income_data: [], // 收入数组\r\n      totalType: 'expense', // 收支类型切换变量\r\n      pieType: 'expense', // icon 对象\r\n      payType: 'expense'\r\n    })\r\n\r\n    onMounted(() => {\r\n      getData()\r\n    })\r\n\r\n    onUnmounted(() => {\r\n      // 每次组件卸载的时候，需要释放图表实例。clear 只是将其清空不会释放。\r\n      proportionChart.dispose()\r\n    })\r\n    // 获取当月数据接口\r\n    const getData = async () => {\r\n      const { data } = await axios.get(`/bill/data?date=${state.currentMonth}`)\r\n      console.log(data)\r\n      // 总收支\r\n      state.total_expense = data.total_expense\r\n      state.total_income = data.total_income\r\n\r\n      // 过滤支出和收入\r\n      state.expense_data = data.total_data.filter(item => item.pay_type == 1).sort((a, b) => b.number - a.number) // 过滤出账单类型为支出的项\r\n      state.income_data = data.total_data.filter(item => item.pay_type == 2).sort((a, b) => b.number - a.number) // 过滤出账单类型为收入的项\r\n      // 重绘饼图\r\n      setPieChart()\r\n    }\r\n\r\n    // 切换收支数据\r\n    const changeData = (type) => {\r\n      state.payType = type\r\n      state.totalType = type\r\n      state.pieType = type\r\n      // 重绘饼图\r\n      setPieChart()\r\n    }\r\n\r\n    // 切换收支构成类型\r\n    const changeTotalType = (type) => {\r\n      state.totalType = type;\r\n    }\r\n\r\n    // 切换饼图收支类型\r\n    const changePieType = (type) => {\r\n      state.pieType = type\r\n      // 重绘饼图\r\n      setPieChart()\r\n    }\r\n\r\n    // 绘制饼图方法\r\n    const setPieChart = () => {\r\n      if (window.echarts) {\r\n        // 初始化\r\n        proportionChart = echarts.init(document.getElementById('proportion'));\r\n        // 根据 pieType 变量判断是支出还是收入\r\n        const _data = state.pieType == 'expense' ? state.expense_data : state.income_data\r\n        // 添加图标配置数据\r\n        proportionChart.setOption({\r\n            tooltip: {\r\n              trigger: 'item',\r\n              formatter: '{a} <br/>{b} : {c} ({d}%)'\r\n            },\r\n            // 图例\r\n            legend: {\r\n                data: _data.map(item => item.type_name)\r\n            },\r\n            series: [\r\n              {\r\n                name: '支出',\r\n                type: 'pie', // 图标类型\r\n                radius: '55%',\r\n                // 饼图数据\r\n                data: _data.map(item => {\r\n                  return {\r\n                    value: item.number,\r\n                    name: item.type_name\r\n                  }\r\n                }),\r\n                emphasis: {\r\n                  itemStyle: {\r\n                    shadowBlur: 10,\r\n                    shadowOffsetX: 0,\r\n                    shadowColor: 'rgba(0, 0, 0, 0.5)'\r\n                  }\r\n                }\r\n              }\r\n            ]\r\n        })\r\n      }\r\n    }\r\n    \r\n    // 月份弹窗开关\r\n    const monthToggle = () => {\r\n      popMonthRef.value.toggle()\r\n    }\r\n    // 选择月份后的回调方法，拿到月份数据\r\n    const selectMonth = (item) => {\r\n      state.currentMonth = item\r\n      getData()\r\n    }\r\n\r\n    return {\r\n      ...toRefs(state),\r\n      popMonthRef,\r\n      selectMonth,\r\n      monthToggle,\r\n      changeData,\r\n      changeTotalType,\r\n      changePieType\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang='less' scoped>\r\n  @import url('../config/custom.less');\r\n  .data {\r\n    min-height: 100%;\r\n    background-color: #f5f5f5;\r\n    .total {\r\n      background-color: #fff;\r\n      display: flex;\r\n      flex-direction: column;\r\n      align-items: center;\r\n      padding: 24px 0;\r\n      margin-bottom: 10px;\r\n      .time {\r\n        position: relative;\r\n        width: 96px;\r\n        padding: 6px;\r\n        background-color: #f5f5f5;\r\n        color: @color-text-base;\r\n        border-radius: 4px;\r\n        font-size: 14px;\r\n        display: flex;\r\n        justify-content: space-between;\r\n        align-items: center;\r\n        margin-bottom: 16px;\r\n        span:nth-of-type(1)::after {\r\n           content: '';\r\n          position: absolute;\r\n          top: 9px;\r\n          bottom: 8px;\r\n          right: 28px;\r\n          width: 1px;\r\n          background-color: rgba(0, 0, 0, .5);\r\n        }\r\n        .van-icon-notes-o {\r\n          font-size: 16px;\r\n          color: @color-text-caption;\r\n        }\r\n      }\r\n      .box {\r\n        display: flex;\r\n        flex-direction: column;\r\n        justify-content: space-between;\r\n        align-items: center;\r\n        .title_expense {\r\n          color: @primary;\r\n          margin-bottom: 8px;\r\n          font-size: 12px;\r\n          font-weight: 500;\r\n        }\r\n        .title_income {\r\n          color: @text-warning;\r\n          margin-bottom: 8px;\r\n          font-size: 12px;\r\n          font-weight: 500;\r\n        }\r\n        .money_expense {\r\n          font-size: 28px;\r\n          color: @primary;\r\n          font-weight: 600;\r\n          margin-bottom: 16px;\r\n        }\r\n        .money_income {\r\n          font-size: 28px;\r\n          color: @text-warning;\r\n          font-weight: 600;\r\n          margin-bottom: 16px;\r\n        }\r\n        .income {\r\n          color: @color-text-caption;\r\n          font-weight: 500;\r\n        }\r\n      }\r\n    }\r\n    .structure {\r\n      padding: 0 16px;\r\n      background-color: #fff;\r\n      margin-bottom: 10px;\r\n      .head {\r\n        display: flex;\r\n        justify-content: space-between;\r\n        align-items: center;\r\n        padding: 12px 0;\r\n        .title {\r\n          font-size: 18px;\r\n          color: @color-text-base;\r\n        }\r\n        .tab {\r\n          span {\r\n            display: inline-block;\r\n            width: 40px;\r\n            height: 24px;\r\n            background-color: #f5f5f5;\r\n            text-align: center;\r\n            line-height: 24px;\r\n            margin-left: 10px;\r\n            border-radius: 4px;\r\n          }\r\n          .expense {\r\n            &.active {\r\n              background-color: rgba(250,227,217,0.2);\r\n              color: @primary;\r\n            }\r\n          }\r\n          .income {\r\n            &.active {\r\n              background-color: #fbf8f0;\r\n              color: @text-warning;\r\n            }\r\n          }\r\n        }\r\n      }\r\n      .content {\r\n        .item {\r\n          display: flex;\r\n          height: 50px;\r\n          align-items: center;\r\n          .left {\r\n            flex: 4;\r\n            display: flex;\r\n            align-items: center;\r\n            justify-content: space-between;\r\n            margin-right: 10px;\r\n            .type {\r\n              display: flex;\r\n              align-items: center;\r\n              span:nth-of-type(1) {\r\n                display: flex;\r\n                justify-content: center;\r\n                align-items: center;\r\n                border-radius: 50%;\r\n                width: 30px;\r\n                height: 30px;\r\n                margin-right: 10px;\r\n                color: #fff;\r\n              }\r\n              .expense {\r\n                background-color: @primary;\r\n              }\r\n              .income {\r\n                background-color: rgb(236, 190, 37);\r\n              }\r\n            }\r\n          }\r\n          .right {\r\n            flex: 8;\r\n            display: flex;\r\n            align-items: center;\r\n            .percent {\r\n              width: 160px;\r\n            }\r\n            .momey {\r\n              width: 100px;\r\n            }\r\n          }\r\n        }\r\n      }\r\n    }\r\n    .proportion {\r\n      background-color: #fff;\r\n      padding: 12px;\r\n      .head {\r\n        display: flex;\r\n        justify-content: space-between;\r\n        align-items: center;\r\n        padding: 12px 0;\r\n        .title {\r\n          font-size: 18px;\r\n          color: @color-text-base;\r\n        }\r\n        .tab {\r\n          span {\r\n            display: inline-block;\r\n            width: 40px;\r\n            height: 24px;\r\n            background-color: #f5f5f5;\r\n            text-align: center;\r\n            line-height: 24px;\r\n            margin-left: 10px;\r\n            border-radius: 4px;\r\n          }\r\n          .expense {\r\n            &.active {\r\n              background-color: rgba(255,247,247,1);\r\n              color: @primary;\r\n            }\r\n          }\r\n          .income {\r\n            &.active {\r\n              background-color: #fbf8f0;\r\n              color: @text-warning;\r\n            }\r\n          }\r\n        }\r\n      }\r\n      #proportion {\r\n        width: 100%;\r\n        height: 400px;\r\n      }\r\n    }\r\n  }\r\n</style>"],"sourceRoot":""}]}